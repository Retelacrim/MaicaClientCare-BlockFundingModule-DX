        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Funding_Period_End_Date_Current_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Period_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
